{
  "name": "web-app",
  "version": "1.0.0",
  "description": "CRUD web application with Express and SQLite3",
  "main": "dist/app.js",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon src/app.ts",
    "build": "tsc && tsc -p tsconfig.frontend.json",
    "watch": "tsc -w",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.17.9",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
